        var SupplierName = $('#txtSearch').val()
        if (SupplierName == "") {
            GetSupplierDetails();
        }
        $.ajax({
            url: rootUrl + "/api/TestApi/SearchSupplierBySupplierName?Search=" + SupplierName,
            type: "Get",
            contentType: "application/json; charset=utf-8",
            success: function (response) {
                BindSupplierDetails(response);
            }
            
        });
    }
    function GotoLogin() {
        try {
            window.location.href = "/Login/Login";
        }
        catch (e) {
        }
    }
</script>

