                for (Supplier = 0; Supplier < response.length; Supplier++) {

                    $('#txtSupplierId').val(SupplierID);
                    $('#txtSupplierName').val(response[Supplier].PH_SupplierName);
                    $('#txtAddress1').val(response[Supplier].PH_Supplier_Address1);
                    $('#txtAddress2').val(response[Supplier].PH_Supplier_Address2);
                    $('#txtCity').val(response[Supplier].CityName);
                    //$('#txtCityId').val(response[Supplier].PH_SupplierName);
                    $('#txtState').val(response[Supplier].StateName);
                    $('#txtCountry').val(response[Supplier].CountryName);
                    $('#txtTelephoneNo1').val(response[Supplier].PH_Supplier_Office_Contact1);
                    $('#txtTelephoneNo2').val(response[Supplier].PH_Supplier_Office_Contact2);
                    $('#txtEmailId').val(response[Supplier].PH_Supplier_EmailId);
                    $('#txtGSTRegNo').val(response[Supplier].PH_Supplier_GSTRegNo);
                    $('#txtUrl').val(response[Supplier].PH_Supplier_Url);
                    $('#txtCityId').val(response[Supplier].PH_Supplier_CityId);
                    $('#txtStateId').val(response[Supplier].PH_Supplier_StateId);
                    $('#txtCountrySeqId').val(response[Supplier].PH_Supplier_CountryId);
                    $('#btnUpdate').attr('hidden', false);
                    $('#btnSave').attr('hidden', true);
                }
            }
        });



    }


    function UpdateContactDetails() {
        var SuppierId = $('#txtSuppierId').val();
        var SuppierID = parseFloat(SuppierId)
        var ContactDetailId = $('#txContactDetailId').val();
        var ContactId = parseFloat(ContactDetailId)
        var ContactName = $('#txtContactName').val();
        var Designation = $('#txtDesignation').val();
        var MobileNo1 = $('#txtMobileNo1').val();
        var MobileNo2 = $('#txtMobileNo2').val();
        var ContactEmailId = $('#txtContactEmailId').val();
        var UpdateContact = {
            PH_SupplierContact_Person: ContactName,
            PH_SupplierContact_Designation: Designation,
            PH_SupplierContact_Person_Phone: MobileNo1,
            PH_SupplierContact_Person_Phone2: MobileNo2,
            PH_SupplierContact_Person_EmailId: ContactEmailId,
            PH_ContactDetailId: ContactId,
            PH_SupplierID: SuppierID
        }
        $.ajax({
            url: rootUrl + "/api/TestApi/UpdateContactDetails",
            type: "POST",
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify(UpdateContact),
            dataType: 'json',
            success: function (response) {
                if (response > 0) {
                    alert("Updated Sucessfully");
                    OnContactCancel();
                    var SupplierId = $('#txtSupplierId').val();
                    BindContactDetails(SupplierId);
                }
            },
            failure: function (response) {
                alert(response);
            },
            error: function (response) {
                alert(response);
            }
        });

    }
