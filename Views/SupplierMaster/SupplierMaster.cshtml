    function BindContactData(PH_ContactDetailId) {
        $.ajax({
            url: rootUrl + "/api/TestApi/GetSupplierContactDetailsById?PH_ContactDetailId=" + PH_ContactDetailId,
            type: 'Get',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
                $('#modal-default').modal('show');
                for (var Supplier = 0; Supplier < response.length; Supplier++) {
                    $('#txContactDetailId').val(PH_ContactDetailId);
                    $('#txtContactName').val(response[Supplier].PH_SupplierContact_Person);
                    $('#txtDesignation').val(response[Supplier].PH_SupplierContact_Designation);
                    $('#txtMobileNo1').val(response[Supplier].PH_SupplierContact_Person_Phone);
                    $('#txtMobileNo2').val(response[Supplier].PH_SupplierContact_Person_Phone2);
                    $('#txtContactEmailId').val(response[Supplier].PH_SupplierContact_Person_EmailId);
                    $('#txtSuppierId').val(response[Supplier].PH_SupplierID);
                    $('#btnCntupdate').attr('hidden', false);
                    $('#btnCntSave').attr('hidden', true);

                }

            }
        });



    }




