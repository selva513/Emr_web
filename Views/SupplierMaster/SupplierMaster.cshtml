    function BindSupplierDetails(response) {
                var html = "";
                $("#tblSupplier tbody").empty();
                for (Supplier = 0; Supplier < response.length; Supplier++) {
                    var EmailId = response[Supplier].PH_Supplier_EmailId;
                    if (EmailId == null) {
                        EmailId = "";
                    }
                    var CityName = response[Supplier].CityName;
                    if (CityName == null) {
                        CityName = "";
                    }
                    var CountryName = response[Supplier].CountryName;
                    if (CountryName == null) {
                        CountryName = "";
                    }
                    var StateName = response[Supplier].StateName
                    if (StateName == null) {
                        StateName = "";
                    }
                   
                    var SupplierId = response[Supplier].PH_SupplierID;
                    $('#txtSupplierId').val(SupplierId);
                    html += "<tr  onclick='GetContactDetails(this,event)'>";
                    html += "<td style='display:none'>" + SupplierId + "</td>";
                    html += "<td>" + response[Supplier].PH_SupplierName + "</td>";
                    html += "<td>" + response[Supplier].PH_Supplier_Address1 + "</td>";
                    html += "<td>" + CityName + "</td>";
                    html += "<td>" + StateName + "</td>";
                    html += "<td>" + CountryName + "</td>";
                    html += "<td>" + EmailId + "</td>";
                    html += "<td > <input type='image' src='/images/Edit1.png' style='height:20px' onclick='GetScreenSelect(this,event);' />" +
                        "<input type='image' src='/images/Delete1.jpg' style='height:25px;margin-left:3px;margin-left: 12px;' onclick='DeleteSupplierPopup();' /></td >";
                    html += "<tr>";
                }
                $("#tblSupplier tbody").append(html);
        
           


  


    }

