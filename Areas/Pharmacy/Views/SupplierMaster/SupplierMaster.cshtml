    function UpdateContactDetails() {
        var SuppierId = $('#txtSuppierId').val();
        var SuppierID = parseFloat(SuppierId)
        var ContactDetailId = $('#txContactDetailId').val();
        var ContactId = parseFloat(ContactDetailId)
        var ContactName = $('#txtContactName').val();
        var Designation = $('#txtDesignation').val();
        var MobileNo1 = $('#txtMobileNo1').val();
        var MobileNo2 = $('#txtMobileNo2').val();
        var ContactEmailId = $('#txtContactEmailId').val();
        var UpdateContact = {
            PH_SupplierContact_Person: ContactName,
            PH_SupplierContact_Designation: Designation,
            PH_SupplierContact_Person_Phone: MobileNo1,
            PH_SupplierContact_Person_Phone2: MobileNo2,
            PH_SupplierContact_Person_EmailId: ContactEmailId,
            PH_ContactDetailId: ContactId,
            PH_SupplierID: SuppierID
        }
        $.ajax({
            url: rootUrl + "/api/TestApi/UpdateContactDetails",
            type: "POST",
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify(UpdateContact),
            dataType: 'json',
            success: function (response) {
                if (response > 0) {
                    alert("Updated Sucessfully");
                    OnContactCancel();
                    var SupplierId = $('#txtSupplierId').val();
                    BindContactDetails(SupplierId);
                }
            },
            failure: function (response) {
                alert(response);
            },
            error: function (response) {
                alert(response);
            }
        });

    }
    function OnContactCancel() {

        $('#txContactDetailId').val('');
        $('#txtContactName').val('');
        $('#txtDesignation').val('');
        $('#txtMobileNo1').val('');
        $('#txtMobileNo2').val('');
        $('#txtContactEmailId').val('');
        $('#txtSuppierId').val('');
        $('#btnCntupdate').prop('hidden', true);
        $('#btnCntSave').prop('hidden', false);
    }
    function SaveContactDetails() {
        var SuppierId = $('#txtSupplierId').val();
        var SuppierID = parseFloat(SuppierId)
        var ContactName = $('#txtContactName').val();
        var Designation = $('#txtDesignation').val();
        var MobileNo1 = $('#txtMobileNo1').val();
        var MobileNo2 = $('#txtMobileNo2').val();
        var ContactEmailId = $('#txtContactEmailId').val();
        var SaveContact = {
            PH_SupplierContact_Person: ContactName,
            PH_SupplierContact_Designation: Designation,
            PH_SupplierContact_Person_Phone: MobileNo1,
            PH_SupplierContact_Person_Phone2: MobileNo2,
            PH_SupplierContact_Person_EmailId: ContactEmailId,
            PH_SupplierID: SuppierID
        }
        $.ajax({
            url: rootUrl + "/api/TestApi/CreateContactDetails",
            type: "POST",
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify(SaveContact),
            dataType: 'json',
            success: function (response) {
                if (response > 0) {
                    alert("Saved Sucessfully");
                    OnContactCancel();
                    var SupplierId = $('#txtSupplierId').val();
                    BindContactDetails(SupplierId)
                }
            },
            failure: function (response) {
                alert(response);
            },
            error: function (response) {
                alert(response);
            }
        });

    }

