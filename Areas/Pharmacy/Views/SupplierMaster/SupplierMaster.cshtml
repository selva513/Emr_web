                var Stateid = $('#txtSupplierStateId').val();
                var StateId = parseFloat(Stateid);
                var Country = ($('#txtSupplierCountry').val()).trim();;
                var Countryid = $('#txtSupplierCountrySeqId').val();
                var CountryId = parseFloat(Countryid);
                var TelephoneNo1 = ($('#txtSupplierTelephoneno1').val()).trim();
                var TelephoneNo2 = ($('#txtSupplierTelephoneno2').val()).trim();
                var EmailId = ($('#txtSupplierEmailId').val()).trim();
                var GSTRegNo = ($('#txtSupplierGSTRegNo').val()).trim();
                var Url = ($('#txtSupplierUrl').val()).trim();
                var ContactName = ($('#txtContactName').val()).trim();
                var Designation = ($('#txtDesignation').val()).trim();
                var MobileNo1 = ($('#txtMobileNo1').val()).trim();
                var MobileNo2 = ($('#txtMobileNo2').val()).trim();
                var ContactEmailId = ($('#txtContactEmailId').val()).trim();
                var CityPin = $('#imgCityPin').val();

                if (Address1 != null) {
                    Address1 = Address1.trim();
                }
                else {
                    Address1 = "";
                }
                if (Address2 != null) {
                    Address2 = Address2.trim();
                }
                else {
                    Address2 = "";
                }

                $.ajax({
                    url: rootUrl + "/api/TestApi/IsSupplierExists",
                    type: 'Get',
                    contentType: "application/json; charset=utf-8",
                    data: { SupplierName: SupplierName, CityName: City, GstNo: GSTRegNo, Address1: Address1, Address2: Address2, SupplierId: SupplierId },
                    dataType: "json",
                    success: function (response) {
                        if (response) {
                            $('#btnSupplierUpdate').prop("disabled", false);
                            txt = "Supplier Already Exists";
                            alert(txt);
                            return false;
                        }
                        else {
                            var UpdateSupplier = {
                                PH_SupplierID: SupplierId,
                                PH_SupplierName: SupplierName,
                                PH_Supplier_Address1: Address1,
                                PH_Supplier_Address2: Address2,
                                PH_Supplier_State: State,
                                PH_Supplier_Office_Contact1: TelephoneNo1,
                                PH_Supplier_Office_Contact2: TelephoneNo2,
                                PH_Supplier_EmailId: EmailId,
                                PH_Supplier_GSTRegNo: GSTRegNo,
                                PH_Supplier_Url: Url,
                                PH_SupplierContact_Person: ContactName,
                                PH_SupplierContact_Designation: Designation,
                                PH_SupplierContact_Person_Phone: MobileNo1,
                                PH_SupplierContact_Person_Phone2: MobileNo2,
                                PH_SupplierContact_Person_EmailId: ContactEmailId,
                                //PH_Supplier_CityId: CityId,
                                //PH_Supplier_CountryId: CountryId,
                                //PH_Supplier_StateId: StateId,
                                CountryName: Country,
                                CityName: City

                            };

                            $.ajax({
                                url: rootUrl + "/api/TestApi/UpdateSupplierMaster",
                                type: "POST",
                                contentType: "application/json; charset=utf-8",
                                data: JSON.stringify(UpdateSupplier),
                                dataType: 'json',
                                success: function (data) {
                                    if (data == true) {
                                        alert("Updated Sucessfully");
                                        SearchSupplierBySupplierName();
                                        $("#btnSupplierUpdate").prop('disabled', false);
                                        $('#modal-Supplier').modal('hide');


                                        //window.location.href = "/Pharmacy/PharmacyMasters/PharmacyMastersView";
                                    }
                                },
                                failure: function (response) {
                                    alert(response);
                                },
                                error: function (response) {
                                    alert(response);
                                }
                            });
                        }
                    }

                });

            }
            else {
                $("#btnSupplierUpdate").prop('disabled', false);
            }
        }
        catch (e) { console.log(e); }

    }
