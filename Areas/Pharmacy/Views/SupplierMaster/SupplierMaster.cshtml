    function UpdateSupplierMaster() {
        try {
            $("#btnSupplierUpdate").prop('disabled', true);
            if (SupplierSaveValidation()) {
                var txt = "";
                var SupplierID = $('#txtSupplierId').val();
                var SupplierId = parseFloat(SupplierID);
                var SupplierName = ($('#txtSupplierName').val()).trim();
