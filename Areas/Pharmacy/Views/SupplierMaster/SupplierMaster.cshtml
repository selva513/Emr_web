    function SupplierSaveValidation() {
        try {
            var txt = "Required to fill the following field(s)";
            var opt = 0;
            var SupplierName = $('#txtSupplierName').val();
            var Address1 = $('#txtSupplierAddress1').val();
            var Address2 = $('#txtSupplierAddress2').val();
            var City = $('#txtSupplierCity').val();
            var State = $('#txtSupplierState').val();
            var Country = $('#txtSupplierCountry').val();
            var TelephoneNo1 = $('#txtSupplierTelephoneno1').val();
            var TelephoneNo2 = $('#txtSupplierTelephoneno2').val();
            var EmailId = $('#txtSupplierEmailId').val();
            var GSTRegNo = $('#txtSupplierGSTRegNo').val();
            var Url = $('#txtSupplierUrl').val();

            if (SupplierName == "" || SupplierName == null) {
                txt += "\n - Please Enter Supplier Name";
                opt = 1;
            }
            if (City == "" || City == null) {
                txt += "\n - Please Enter City";
                opt = 1;
            }
            if (State == "" || State == null) {
                txt += "\n - Please Enter State";
                opt = 1;
            }
            if (Country == "" || Country == null) {
                txt += "\n - Please Enter Country";
                opt = 1;
            }
            if (GSTRegNo == "" || GSTRegNo == null) {
                txt += "\n - Please Enter Gst Regn No";
                opt = 1;
            }

            if (opt == 1) {
                alert(txt);
                return false;
            }

            return true;
        }
        catch (e) { console.log(e); }
    }
