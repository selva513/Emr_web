    function BindSupplierDetailsBySeqId(SupplierId) {
        var SupplierID = SupplierId;
        $.ajax({
            url: rootUrl + "/api/TestApi/GetSupplierDetailsBySeqId?SupplierId=" + SupplierId,
            type: 'Get',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {

                for (Supplier = 0; Supplier < response.length; Supplier++) {

                    $('#txtSupplierId').val(SupplierID);
                    $('#txtSupplierName').val(response[Supplier].PH_SupplierName);
                    $('#txtSupplierAddress1').val(response[Supplier].PH_Supplier_Address1);
                    $('#txtSupplierAddress2').val(response[Supplier].PH_Supplier_Address2);
                    $('#txtSupplierCity').val(response[Supplier].CityName);
