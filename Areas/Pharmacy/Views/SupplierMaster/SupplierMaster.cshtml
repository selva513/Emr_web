    function BindContactDetails(SupplierId) {
        var SupplierID = SupplierId;
        $('#txtSupplierId').val(SupplierID);
        var html = "";
        $("#ContactDetails tbody").empty();
        $.ajax({
            url: rootUrl + "/api/TestApi/GetSupplierContactDetails?SupplierId=" + SupplierId,
            type: 'Get',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
                for (Supplier = 0; Supplier < response.length; Supplier++) {
                    html += "<tr>";
                    html += "<td style='display:none'>" + response[Supplier].PH_ContactDetailId + "</td>";
                    //html += "<td  >" + response[Supplier].PH_SupplierName + "</td>";
                    html += "<td>" + response[Supplier].PH_SupplierContact_Person + "</td>";
                    html += "<td>" + response[Supplier].PH_SupplierContact_Person_Phone + "</td>";
                    html += "<td>" + response[Supplier].PH_SupplierContact_Designation + "</td>";
                    html += "<td>" + response[Supplier].PH_SupplierContact_Person_EmailId + "</td>";
                    html += "<td> <input type='image' src='/images/Edit1.png' style='height:20px' onclick ='BindUpdateDetails(this);' />" +
                        "<input type='image' src='/images/Delete1.jpg' style='height:25px;margin-left:3px;margin-left: 12px;' onclick='DeleteConatctPopup(this);' /></td >";
                    html += "<td style='display:none'>" + SupplierID + "</td>";
                    html += "<tr>";
                }
                $("#ContactDetails tbody").append(html);
            }

        });

    }
